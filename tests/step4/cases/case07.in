#!/bin/bash

input_string="goodbye"

case $input_string in
    # match a string that starts with "hello"
    hello*)
        echo "The string starts with 'hello'"
        ;;

    # match a string that is exactly "goodbye"
    goodbye)
        echo "The string is exactly 'goodbye'"
        ;;

    # match any other string
    *)
        echo "The string does not match any of the other cases"
        ;;
esac
